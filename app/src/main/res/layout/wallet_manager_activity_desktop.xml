<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.bitdubai.smartwallet.BlankActivity"
    android:background="@drawable/background_tiled_diagonal_light">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="14dp"
        android:paddingRight="14dp"
        android:orientation="horizontal"
        android:id="@+id/linearLayout">







    </LinearLayout>

    <TableLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">
            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:tag="1"
                android:id="@+id/wallet_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="14dp"
                android:paddingBottom="14dp"
                android:orientation="vertical"
                android:onClick="onWalletClicked">
                <ImageView
                    android:id="@+id/image10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="centerCrop"
                    android:paddingLeft="14dp"
                    android:paddingRight="14dp"
                    android:src="@drawable/icono_piggy_pink"
                    android:layout_alignParentTop="true"
                    android:layout_column="0"
                    />

                <TextView android:text="Girl's saving"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView10"
                    android:layout_below="@+id/image10"
                    android:layout_column="0"
                    android:textColor="@android:color/black"
                    android:gravity="center"/>
            </LinearLayout>

            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:tag="2"
                android:id="@+id/wallet_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="14dp"
                android:paddingBottom="14dp"
                android:orientation="vertical"
                android:onClick="onWalletClicked">
                <ImageView
                    android:id="@+id/image11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="centerCrop"
                    android:paddingLeft="14dp"
                    android:paddingRight="14dp"
                    android:src="@drawable/icono_piggy_yellow"
                    android:layout_alignParentTop="true"
                    android:layout_column="1" />

                <TextView android:text="Boy's savings"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView11"
                    android:layout_below="@+id/image11"
                    android:layout_centerHorizontal="true"
                    android:layout_column="1"
                    android:textColor="@android:color/black"
                    android:gravity="center"/>
            </LinearLayout>

            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:tag="3"
                android:id="@+id/wallet_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="14dp"
                android:paddingBottom="14dp"
                android:orientation="vertical"
                android:onClick="onWalletClicked">
                <ImageView
                    android:id="@+id/image12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="centerCrop"
                    android:paddingLeft="14dp"
                    android:paddingRight="14dp"
                    android:src="@drawable/wallet_1"
                    android:layout_alignParentTop="true"
                    android:layout_column="2" />

                <TextView android:text="Ladies"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView3"
                    android:layout_below="@+id/image12"
                    android:layout_centerHorizontal="true"
                    android:layout_column="2"
                    android:textColor="@android:color/black"
                    android:gravity="center"/>
            </LinearLayout>


            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:tag="4"
                android:id="@+id/wallet_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="14dp"
                android:paddingBottom="14dp"
                android:orientation="vertical"
                android:onClick="onWalletClicked">
                <ImageView
                    android:id="@+id/image13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="centerCrop"
                    android:paddingLeft="14dp"
                    android:paddingRight="14dp"
                    android:src="@drawable/wallet_2"
                    android:layout_alignParentTop="true"
                    android:layout_column="0"
                    />

                <TextView android:text="Young"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView13"
                    android:layout_below="@+id/image13"
                    android:layout_column="0"
                    android:textColor="@android:color/black"
                    android:gravity="center"/>
            </LinearLayout>

        </TableRow>


        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">


            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:tag="5"
                android:id="@+id/wallet_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="14dp"
                android:paddingBottom="14dp"
                android:orientation="vertical"
                android:onClick="onWalletClicked">
                <ImageView
                    android:id="@+id/image14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="centerCrop"
                    android:paddingLeft="14dp"
                    android:paddingRight="14dp"
                    android:src="@drawable/wallet_3"
                    android:layout_alignParentTop="true"
                    android:layout_column="1" />

                <TextView android:text="Professional"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView14"
                    android:layout_below="@+id/image14"
                    android:layout_centerHorizontal="true"
                    android:layout_column="1"
                    android:textColor="@android:color/black"
                    android:gravity="center"/>
            </LinearLayout>

            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:tag="6"
                android:id="@+id/wallet_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="14dp"
                android:paddingBottom="14dp"
                android:orientation="vertical"
                android:onClick="onWalletClicked">
                <ImageView
                    android:id="@+id/image15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="centerCrop"
                    android:paddingLeft="14dp"
                    android:paddingRight="14dp"
                    android:src="@drawable/wallet_4"
                    android:layout_alignParentTop="true"
                    android:layout_column="2" />

                <TextView android:text="Gucci"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView14"
                    android:layout_below="@+id/image14"
                    android:layout_centerHorizontal="true"
                    android:layout_column="2"
                    android:textColor="@android:color/black"
                    android:gravity="center"/>
            </LinearLayout>


            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:tag="7"
                android:id="@+id/wallet_6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="14dp"
                android:paddingBottom="14dp"
                android:orientation="vertical"
                android:onClick="onWalletClicked">
                <ImageView
                    android:id="@+id/image1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="centerCrop"
                    android:paddingLeft="14dp"
                    android:paddingRight="14dp"
                    android:src="@drawable/icono_retailer_1"
                    android:layout_alignParentTop="true"
                    android:layout_column="0"
                    />

                <TextView android:text="@string/icono1"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView1"
                    android:layout_below="@+id/image1"
                    android:layout_column="0"
                    android:textColor="@android:color/black"
                    android:gravity="center"/>
            </LinearLayout>

            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:tag="8"
                android:id="@+id/wallet_7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="14dp"
                android:paddingBottom="14dp"
                android:orientation="vertical"
                android:onClick="onWalletClicked">
                <ImageView
                    android:id="@+id/image2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="centerCrop"
                    android:paddingLeft="14dp"
                    android:paddingRight="14dp"
                    android:src="@drawable/icono_banco_1"
                    android:layout_alignParentTop="true"
                    android:layout_column="1" />

                <TextView android:text="@string/icono2"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView2"
                    android:layout_below="@+id/image2"
                    android:layout_centerHorizontal="true"
                    android:layout_column="1"
                    android:textColor="@android:color/black"
                    android:gravity="center"/>
            </LinearLayout>

        </TableRow>


        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:tag="9"
                android:id="@+id/wallet_8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="14dp"
                android:paddingBottom="14dp"
                android:orientation="vertical"
                android:onClick="onWalletClicked">
                <ImageView
                    android:id="@+id/image3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="centerCrop"
                    android:paddingLeft="14dp"
                    android:paddingRight="14dp"
                    android:src="@drawable/icono_banco_2"
                    android:layout_alignParentTop="true"
                    android:layout_column="2" />

                <TextView android:text="@string/icono3"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView3"
                    android:layout_below="@+id/image3"
                    android:layout_centerHorizontal="true"
                    android:layout_column="2"
                    android:textColor="@android:color/black"
                    android:gravity="center"/>
            </LinearLayout>



            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:tag="10"
                android:id="@+id/wallet_9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="14dp"
                android:paddingBottom="14dp"
                android:orientation="vertical"
                android:onClick="onWalletClicked">
                <ImageView
                    android:id="@+id/image4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="centerCrop"
                    android:paddingLeft="14dp"
                    android:paddingRight="14dp"
                    android:src="@drawable/icono_club_1"
                    android:layout_alignParentTop="true"
                    android:layout_column="0" />

                <TextView android:text="@string/icono4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView4"
                    android:layout_below="@+id/image4"
                    android:layout_centerHorizontal="true"
                    android:layout_column="0"
                    android:textColor="@android:color/black"
                    android:gravity="center"/>
            </LinearLayout>

            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:tag="11"
                android:id="@+id/wallet_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="14dp"
                android:paddingBottom="14dp"
                android:orientation="vertical"
                android:onClick="onWalletClicked">
                <ImageView
                    android:id="@+id/image5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="centerCrop"
                    android:paddingLeft="14dp"
                    android:paddingRight="14dp"
                    android:src="@drawable/icono_club_2"
                    android:layout_alignParentTop="true"
                    android:layout_column="1" />

                <TextView android:text="@string/icono5"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView5"
                    android:layout_below="@+id/image5"
                    android:layout_centerHorizontal="true"
                    android:layout_column="1"
                    android:textColor="@android:color/black"
                    android:gravity="center"/>
            </LinearLayout>

        </TableRow>
    </TableLayout>


</RelativeLayout>
