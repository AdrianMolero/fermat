                android:textSize="12sp"
                android:textStyle="bold"
                android:text="Roger"

                android:background="@color/black_overlay"
                android:layout_gravity="center"

                android:paddingLeft="8dip"
                android:paddingRight="8dip"
                />
        </FrameLayout>
    </RelativeLayout>

